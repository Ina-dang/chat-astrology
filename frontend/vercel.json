{
  "builds": [
    {
      "src": "./vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "./dist"
      }
    }
  ],
  "routes": [
    {
      "src": "./api/(.*)",
      "dest": "./api/$1"
    },
    {
      "src": "./(.*)",
      "dest": "./dist/$1"
    }
  ]
}
